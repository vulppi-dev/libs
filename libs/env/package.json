{
  "name": "@vulppi/env",
  "version": "0.0.6",
  "description": "A ENV utilities for Vulppi",
  "license": "MIT",
  "private": false,
  "author": {
    "name": "Renato Rodrigues",
    "email": "renato@vulppi.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vulppi-dev/libs.git",
    "directory": "libs/env"
  },
  "keywords": [
    "vulppi",
    "text",
    "tools",
    "utils",
    "helpers",
    "env",
    "environment",
    "envs",
    "environmental",
    "environments"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "node": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch --noEmit true --emitDeclarationOnly false",
    "build": "bun ./scripts/build index",
    "clear": "rimraf dist",
    "pub": "npm publish --access=public --tag=latest"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "^0.23.0",
    "esbuild-plugin-tsc": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.1.0"
  }
}
