  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vulppi-dev/libs.git",
    "directory": "libs/template"
  },
  "keywords": [
    "tree",
    "node"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "bin": {
    "vp-build": "bin/builder.mjs"
  },
  "scripts": {
    "build": "run-s clear build:all",
