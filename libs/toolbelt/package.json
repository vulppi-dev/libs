    "pub:beta": "npm publish --access public --tag beta"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.4.1",
    "scripts": "workspace:*",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "esbuild": "^0.17.15",
    "esbuild-node-externals": "^1.7.0",
    "minimist": "^1.2.8"
  }
