{
  "name": "@vulppi/string",
  "version": "0.0.2",
  "description": "A string library for Vulppi",
  "license": "MIT",
  "private": false,
  "author": {
    "name": "Renato Rodrigues",
    "email": "renato@vulppi.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vulppi-dev/libs.git",
    "directory": "libs/string"
  },
  "keywords": [
    "vulppi",
    "text",
    "tools",
    "utils",
    "helpers",
    "string",
    "inject",
    "injection",
    "interpolation",
    "interpolate",
    "template",
    "templates",
    "format",
    "formatting",
    "replace",
    "replacing",
    "replaceall",
    "replace-all",
    "validate",
    "validation",
    "is",
    "regexp",
    "regex",
    "regexes",
    "regular",
    "expression",
    "expressions",
    "match",
    "matching",
    "matches"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "node": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch --noEmit true --emitDeclarationOnly false",
    "build": "bun ./scripts/build index",
    "clear": "rimraf dist",
    "pub": "npm publish --access=public --tag=latest"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.14.10",
    "esbuild": "^0.23.0",
    "rimraf": "^6.0.1",
