{
  "name": "@vulppi/node-tree",
  "version": "0.4.4",
  "description": "A simple tree data structure for Node.js",
  "license": "MIT",
  "private": false,
  "author": {
    "name": "Renato Rodrigues",
    "email": "renato@vulppi.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vulppi-dev/libs.git",
    "directory": "libs/node-tree"
  },
  "keywords": [
    "tree",
    "node"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "node": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch --noEmit true --emitDeclarationOnly false",
    "build": "bun ./scripts/build index",
    "clear": "rimraf dist",
    "pub": "npm publish --access=public --tag=latest"
  },
  "devDependencies": {
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.10",
    "chai": "^5.1.1",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "^0.23.0",
    "mocha": "^10.6.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
