{
  "name": "@vulppi/env",
  "version": "0.0.7",
  "description": "A ENV utilities for Vulppi",
  "license": "MIT",
  "private": false,
  "author": {
    "name": "Renato Rodrigues",
    "email": "renato@vulppi.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vulppi-dev/libs.git",
    "directory": "libs/env"
  },
  "keywords": [
    "vulppi",
    "text",
    "tools",
    "utils",
    "helpers",
    "env",
    "environment",
    "envs",
    "environmental",
    "environments"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsc --watch --noEmit true --emitDeclarationOnly false",
    "build": "vpp build index",
    "pub": "npm publish --access=public --tag=latest"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "typescript": "^5.7.2",
    "vulppi": "^0.2.9"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.1.0"
  }
}
